<script>
function toggleAIBot() {
  var bot = document.getElementById('ai-bot');
  bot.style.display = (bot.style.display === 'none' || bot.style.display === '') ? 'block' : 'none';
  if(bot.style.display === 'block') setTimeout(()=>{document.getElementById('ai-input').focus()},300);
}
function aiSendMsg(e){
  e.preventDefault();
  var input = document.getElementById('ai-input');
  var chat = document.getElementById('ai-chat');
  var msg = input.value.trim();
  if(!msg) return;
  chat.innerHTML += '<div style="color:#222;text-align:right;margin-bottom:4px;">๐งโ๐ผ '+msg+'</div>';
  // ุฑุฏูุฏ ุฐููุฉ ูุจุณุทุฉ
  var reply = '';
  if(msg.includes('ุชุชุจุน')) reply = 'ููููู ุชุชุจุน ุดุญูุชู ุนุจุฑ ูุณู "ุชุชุจุน ุดุญูุชู" ุฃู ุชุฒููุฏู ุจุฑูู ุงูุดุญูุฉ!';
  else if(msg.includes('ุณุนุฑ')||msg.includes('ุชูููุฉ')) reply = 'ููุญุตูู ุนูู ุนุฑุถ ุณุนุฑุ ูุฑุฌู ุชุญุฏูุฏ ููุน ุงูุดุญูุฉ ููุฌูุชูุง ูุณูุฑุฏ ุนููู ููุฑุงู.';
  else if(msg.includes('ุฎุฏูุงุช')||msg.includes('ูุงุฐุง ุชูุฏู')) reply = 'ููุฏู ุดุญู ุฌููุ ุจุญุฑูุ ุจุฑูุ ุชุฎููุต ุฌูุฑููุ ุชุฎุฒููุ ูุชุชุจุน ุฐูู.';
  else if(msg.includes('ุชูุงุตู')||msg.includes('ุฑูู')) reply = 'ููููู ุงูุชูุงุตู ูุนูุง ุนูู: 0097471161164 ุฃู ุนุจุฑ ุฒุฑ ูุงุชุณุงุจ ุงูุนุงุฆู.';
  else reply = 'ุณุคุงูู ููู! ุณูุชู ุชุญูููู ููุฑูู ุงูุฏุนู ุฅุฐุง ูู ุชุฌุฏ ุฅุฌุงุจุชู ููุง.';
  chat.innerHTML += '<div style="color:#25d366;text-align:left;margin-bottom:5px;">๐ค '+reply+'</div>';
